{
	"name": "Rent Cafe Split Assistant",
	"version": "0.0.0",
	"description": "A split rent calculator for RentCafe-powered websites.",
	"homepage_url": "https://github.com/cookiesncache/rent-cafe-split-assistant",
	"manifest_version": 3,
	"minimum_chrome_version": "100",
	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"48": "icons/icon-64.png",
		"128": "icons/icon-128.png",
		"256": "icons/icon-256.png",
		"512": "icons/icon-512.png"
	},
	"host_permissions": [
		"https://*.securecafe.com/"
	],
	"permissions": [
		"activeTab",
		"scripting",
		"storage"
	],
	"action": {
		"default_popup": "options.html"
	},
	"options_ui": {
		"browser_style": true,
		"page": "options.html"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	}
}